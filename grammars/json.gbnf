root ::= object | array
value ::= object | array | string | number | boolean
object ::= "{" ws (string ":" ws value ("," ws string ":" ws value)*)? "}"
array ::= "[" ws (value ("," ws value)*)? "]"
string ::= "\"" [ \t!#-\[\]-~]* "\"" ws
number ::= [0-9]+ ws
boolean ::= ("true" | "false") ws
ws ::= [ \t\t] ws |
